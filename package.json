{
  "name": "terrarium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 vitest run --testTimeout=5000",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "analyze-logos": "tsx scripts/analyzeLogo.ts",
    "ci": "npm run lint && npm run typecheck && npm run test:coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "dotenv -e .env.test -- playwright test --ui --timeout=60000 --workers=1",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.17",
    "@hello-pangea/dnd": "^17.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@supabase/supabase-js": "^2.47.6",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@types/jsonwebtoken": "^9.0.8",
    "jotai": "^2.6.5",
    "jsonwebtoken": "^9.0.2",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.344.0",
    "node-vibrant": "^3.2.1-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.2",
    "sonner": "^1.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^8.57.1",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.17.16",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "uuid": "^11.0.5",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": "prettier --write"
  }
}
